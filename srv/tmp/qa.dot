<!DOCTYPE html>
<html class="bal_out">
	<head>
		<title>{{=it.dat.nam}}</title>
		{{#def.meta}}

		<link rel="stylesheet" type="text/css" href="{{=it.url.rout}}css/s02.css">
		<script type="text/javascript" src="{{=it.url.rout}}js/flashCtrl.js"></script>
		<script type="text/javascript" src="{{=it.url.rout}}js/qa.js"></script>
	</head>
	<body class="bal_out">
		<div id="boso" class="bal_out midOut">
			<div class="midTop"></div><!--
			--><div class="midSub qa_swf">
				<div id="loaDoe" class="qa_loding bfs"></div>
				<div id="tDoe" class="Lc_nosee">
					<div id="qDoe" class="qa_q bfs"></div>
					<div id="aDoe"></div>
					<div id="rDoe" class="qa_r sfs"></div>
					<div id="btnDoe" class="qa_btn bfs" onclick="dco.hid();">确定</div>
				</div>
				<div class="qa_mark">
					<div class="qa_btn sfs" onclick="tls.toggleFullScreen(this, boso);">全屏</div>
				</div>
				<embed id="fDoe" src="{{=it.url.rout}}{{=it.dat.url}}{{=it.dat.file}}"
					wmode="opaque"
					allowscriptaccess="always"
					allowfullscreen="true"
					type="application/x-shockwave-flash"
					pluginspage="http://www.macromedia.com/go/getflashplayer">
				</embed>
			</div>
		</div>
	</body>
	<script>
		tls.rout = "{{=it.url.rout}}";
		dco.path = "{{=it.dat.url}}";
		dco.dat = {{=it.utJson.toJson(it.dat.sub)}};
		document.body.onload = init;
	</script>
</html>
