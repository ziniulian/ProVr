					--><a class="sfs" href="{{=it.link}}" target="_blank">去实验平台登录</a><!--
					--><a class="r sfs" href="{{=it.ilib}}/find/password" target="_blank">忘记密码</a>
				</div>
			</div>
		</div>
	</body>

	<script type="text/javascript">
		dco.rout = "{{=it.url.rout}}";
		dco.bcu = "{{=it.bcu}}";
		document.body.onload = init;
