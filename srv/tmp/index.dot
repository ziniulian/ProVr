<!DOCTYPE html>
<html>
	<head>
		<title>首页</title>
		{{#def.meta}}

		<link rel="stylesheet" href="{{=it.url.rout}}css/s02.css">
		<script type="text/javascript" src="{{=it.url.rout}}js/flashCtrl.js"></script>
		<script type="text/javascript" src="{{=it.url.rout}}js/index.js"></script>
	</head>

	<body>
		<div id="boso">
			<!-- 头 -->
			{{#def.head}}

			<!-- 主体 -->
			<div class="hom_body">
				<div class="hom_w">
					<!-- 视频 -->
					<div class="hom_v_out">
						<div class="hom_v">
							<embed src="{{=it.url.rout}}v/flvplayer.swf"
								wmode="opaque"
								allowscriptaccess="always"
								allowfullscreen="true"
								type="application/x-shockwave-flash"
								flashvars="vcastr_file=a.flv&IsAutoPlay=1&IsContinue=1"
								quality="high"
								width="100%"
								height="100%"
								pluginspage="http://www.macromedia.com/go/getflashplayer">
							</embed>
						</div><!--
						--><div class="hom_v_txt sfs">
							所属分类 ： xxxx<br />
							实验学时 ： xxxx<br />
							实验操作步数 ： xxxx<br />
							实验项目负责人 ： xxxx<br />
							联系电话 ： xxxx<br />
							实验简介 ： xxxx
							<a href="{{=it.url.rout}}ball/">
								<div class="hom_v_btn mfs">开始实验</div>
							</a>
						</div>
					</div>

					<!-- 实验 -->
					<div class="hom_gap bfs">
						<a href="{{=it.url.rout}}home/item/">实验教学项目</a>
					</div>
					<div class="midOut">
						{{~it.dat.hom.item.sub:o:i}}
							<a href="{{=it.url.rout}}home/item/{{=i}}/">
								<div class="hom_test_btn hom_test_btn_{{=i}} sfs">{{=o.nam}}</div>
							</a>
						{{~}}
					</div>

					<!-- 团队 -->
					<div class="hom_gap bfs">
						<a href="{{=it.url.rout}}home/team/">服务团队</a>
					</div>
					<div id="albumDoe" class="hom_team"></div>

					<!-- 网络 -->
					<div class="hom_gap bfs">
						<a href="{{=it.url.rout}}home/net/">网络相关要求</a>
					</div>
					<div class="hom_net">
						{{~it.dat.hom.net.sub:o:i}}
							{{? i%3}}
								<div class="hom_net_gap"></div>
							{{?}}
							<a href="{{=it.url.rout}}home/net/{{=i}}/">
								<div class="hom_net_img hom_net_img_{{=i}}"></div>
								<div class="hom_net_txt sfs">{{= o.anm || o.nam}}</div>
							</a>
						{{~}}
					</div>
				</div>
			</div>

			<!-- 尾 -->
			{{#def.foot}}
		</div>
	</body>

	<script type="text/javascript">
		dco.rout = "{{=it.url.rout}}";
		document.body.onload = dco.init({{=it.utJson.toJson(it.dat.img)}});
	</script>
</html>
